{
  "expo": {
    "name": "Rememberly",
    "slug": "rememberly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rememberly",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.supabase.lkeequxsxsoastroxqnv.rememberly",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "package": "co.supabase.lkeequxsxsoastroxqnv.rememberly",
      "versionCode": 1,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "USE_EXACT_ALARM",
        "SCHEDULE_EXACT_ALARM"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#3B82F6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#3B82F6",
          "defaultChannel": "default",
          "sounds": [],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "rememberly-supabase-notifications"
      }
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#3B82F6",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new reminders"
    }
  }
}